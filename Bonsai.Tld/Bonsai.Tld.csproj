<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Bonsai - TLD Library</Title>
    <Description>Bonsai Tracking Library containing the TLD self-supervised tracking algorithm.</Description>
    <PackageTags>Bonsai Rx Tracking TLD OpenTLD</PackageTags>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <TargetFramework>net462</TargetFramework>
    <VersionPrefix>0.2.0</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bonsai.Core" Version="2.7.0" />
    <PackageReference Include="Bonsai.Vision" Version="2.7.0" />
    <PackageReference Include="OpenTld.Net" Version="0.2.0" />
    <PackageReference Include="OpenTK" Version="3.1.0" />
  </ItemGroup>

</Project>
